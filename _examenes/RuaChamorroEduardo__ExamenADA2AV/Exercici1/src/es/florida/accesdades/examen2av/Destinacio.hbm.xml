<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.accesdades.examen2av">
    <class name="Destinacio" table="destinacions">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="lloc" column="lloc" type="string"/>
        <property name="preu" column="preu" type="int"/>
        <property name="preuOferta" column="preuOferta" type="int"/>
        <property name="passaportCOVID" column="passaportCOVID" type="string"/>
    </class>
</hibernate-mapping>